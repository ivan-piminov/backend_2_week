{
  "version": 2,
  "name": "backend_week_2",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "strict-origin"
        },
        {
          "key" : "Permissions-Policy",
          "value" : "geolocation=(self), microphone=()"
        }
      ]
    }
  ]
}
