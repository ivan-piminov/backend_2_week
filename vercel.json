{
  "version": 2,
  "name": "backend_week_2",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts",
      "headers": {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }
    }
  ]
}
